@model AM.ApplicationCore.Domain.Traitement

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>Traitement</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
